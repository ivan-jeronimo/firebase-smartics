{
  "functions": {
    "source": "functions"
  },
  "hosting": [
    {
      "target": "production-shortlink",
      "site": "smartics-prod",
      "public": "public",
      "rewrites": [
        {
          "source": "/**",
          "function": "redirect"
        }
      ]
    },
    {
      "target": "staging-shortlink",
      "site": "smarticsmapsstage",
      "public": "public",
      "rewrites": [
        {
          "source": "/**",
          "function": "redirect"
        }
      ]
    }
  ]
}
